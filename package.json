{
  "name": "@computable/computablejs",
  "version": "0.1.0",
  "description": "Javascript library for developing with the Computable protocol",
  "author": "Computable Labs",
  "license": "MIT",
  "main": "dist/computable.js",
  "module": "dist/index.js",
  "keywords": [
    "Computable",
    "Computable Labs",
    "Ethereum",
    "Javascript",
    "Typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://www.github.com/computablelabs/computable.js"
  },
  "engines": {
    "node": ">=8.2.1"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "JASMINE_CONFIG_PATH=./spec/support/jasmine.json jasmine-ts",
    "build": "npm run build:clean && tsc -p ./tsbuild.json && cp -r ./src/@types/** ./dist/@types",
    "build:clean": " rm -rf ./dist",
    "build:dry": "tsc -noEmit -p ./tsbuild.json",
    "prepublish": "npm run build:clean && npm run build",
    "test-init": "jasmine-ts init"
  },
  "dependencies": {
    "ethereumjs-tx": "^1.3.7",
    "web3": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "@types/ethereumjs-tx": "^1.0.0",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^10.5.0",
    "@types/underscore": "^1.8.8",
    "@types/web3": "^1.0.10",
    "bignumber.js": "^7.2.1",
    "ganache-cli": "^6.1.0",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "truffle-hdwallet-provider": "0.0.5",
    "typescript": "^2.8.1"
  }
}
