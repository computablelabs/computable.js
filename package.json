{
  "name": "computable",
  "version": "0.1.0",
  "description": "Javascript library for developing with the Computable protocol",
  "main": "dist/computable.js",
  "module": "dist/index.js",
  "scripts": {
    "test": "jasmine-ts ./spec/**/*.spec.ts",
    "build": "npm run build:clean && tsc -p ./tsbuild.json",
    "build:clean": " rm -rf ./dist/abstracts && rm -rf ./dist/constants && rm -rf ./dist/contracts && rm -rf ./dist/deploy && rm -rf ./dist/helpers && rm -rf ./dist/interfaces && rm -rf ./dist/types",
    "build:dry": "tsc -noEmit -p ./tsbuild.json",
    "test-init": "jasmine-ts init"
  },
  "keywords": [
    "Computable",
    "Computable Labs",
    "Ethereum",
    "Javascript",
    "Typescript"
  ],
  "author": "Computable Labs",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/computablelabs/computable.js"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.2.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.6.5",
    "@types/underscore": "^1.8.8",
    "ganache-cli": "^6.1.0",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "truffle-hdwallet-provider": "0.0.5",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "web3": "^1.0.0-beta.34"
  }
}
